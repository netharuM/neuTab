{
    "manifest_version": 2,
    "name": "neuTab",
    "version" : "0.2",
    "web_accessible_resources": [
        "assets/*",
        "dist/*"
    ],
    "chrome_url_overrides":{
        "newtab": "./public/index.html"
    },
    "permissions":[
        "topSites",
        "storage",
        "chrome://favicon/"
    ],
    "content_security_policy": "img-src 'self' data: chrome://favicon;"
}