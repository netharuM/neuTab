{
    "manifest_version": 2,
    "name": "neuTab",
    "version" : "0.3",
    "web_accessible_resources": [
        "assets/*",
        "dist/*"
    ],
    "chrome_url_overrides":{
        "newtab": "./public/index.html"
    },
    "permissions":[
        "topSites",
        "storage",
        "chrome://favicon/"
    ],
    "content_security_policy": "style-src 'unsafe-inline' 'self';default-src 'self';img-src 'self' data: chrome://favicon;"
}